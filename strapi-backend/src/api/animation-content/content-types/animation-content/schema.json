{
  "kind": "collectionType",
  "collectionName": "animation_contents",
  "info": {
    "singularName": "animation-content",
    "pluralName": "animation-contents",
    "displayName": "AnimationContent"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "template": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::animation-template.animation-template",
      "inversedBy": "contents"
    },
    "props": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "published"
      ],
      "default": "draft"
    },
    "renderStatus": {
      "type": "enumeration",
      "enum": [
        "waiting",
        "rendering",
        "done"
      ]
    },
    "renderVideo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "videos"
      ]
    }
  }
}
